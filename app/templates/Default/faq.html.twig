{% extends 'base.html.twig' %}
{% block body %}
   <div class="container content-section">
      {% for g,group in faq %}
          <br>
          <br>
          <br>
         <h2>{{ g }}</h2>
            {% for item in group %}
                <div class="well">
                    <h4 class="panel-title">
                        {{ item.question }}
                    </h4>
                    <br>
                    <p>{{ item.answer }}</p>

                </div>
            {% endfor %}
      {% endfor %}
   </div>
{% endblock %}
