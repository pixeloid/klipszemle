{% extends 'base.html.twig' %}
{% block body %}
   <div class="container content-section">
      {% for g,group in faq %}
          <br>
          <br>
          <br>
         <h2>{{ group.title }}</h2>
            {% for i,item in group.items %}
                <div class="well">
                    <h4 class="panel-title">
                        {{ item.0 }}
                    </h4>
                    <br>
                    <p>{{ item.1 }}</p>

                </div>
            {% endfor %}
      {% endfor %}
   </div>
{% endblock %}
