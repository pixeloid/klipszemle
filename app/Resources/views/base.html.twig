<!DOCTYPE html>
<html>
    <head>
        <title>{% block title %}01. Magyar Klipszemle!{% endblock %}</title>
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <!-- Bootstrap -->
        {% block stylesheets %}
        {% stylesheets
        '@BraincraftedBootstrapBundle/Resources/less/form.less'
        '@PixeloidAppBundle/Resources/public/less/styles.less'
        filter='less, css_url_rewrite' output='css/styles.css' %}
        <link href="{{ asset_url }}" type="text/css" rel="stylesheet" media="all"/>
    {% endstylesheets %}
{% endblock %}
<!-- HTML5 Shim and Respond.js add IE8 support of HTML5 elements and media queries -->
{% include 'BraincraftedBootstrapBundle::ie8-support.html.twig' %}
</head>
<body>

<nav class="navbar navbar-custom navbar-fixed-top" role="navigation">
    <div class="container">
        <div class="navbar-header">
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-main-collapse">
                <i class="fa fa-bars"></i>
            </button>
            <a class="navbar-brand page-scroll" href="/#intro">
            {% image '@PixeloidAppBundle/Resources/public/images/klipszemle_logo.png' %}
                <img src="{{ asset_url }}" class="" height="45" alt=""/>
            {% endimage %}
            </a>
        </div>

        <!-- Collect the nav links, forms, and other content for toggling -->
        <div class="collapse navbar-collapse navbar-right navbar-main-collapse">
        {{ knp_menu_render('PixeloidAppBundle:Builder:mainMenu', {'nav_type': '', 'currentAsLink': true, 'currentClass': 'active'}) }}
        </div>
        <!-- /.navbar-collapse -->
    </div>
    <!-- /.container -->
</nav>



<div class="wrapper">

{% block body %}{% endblock %}

                <div class="push"></div>


</div>
        



         <!-- Footer -->
         <footer>
             <div class="container ">
                <div class="row">
                    <div class="col-sm-4">
                        <ul class="list-unstyled">
                            <li><a href="#">Adatvédelmi szabályzat</a></li>
                            <li><a href="#">Impresszum</a></li>
                        </ul>
                    </div>
                    <div class="col-sm-4 text-center">
                                         <p>Copyright &copy; 2015</p>

                    </div>
                </div>
             </div>
         </footer>




<!-- <script src="https://maps.googleapis.com/maps/api/js?v=3.exp&amp;libraries=places"></script>
 -->{% javascripts  '@jquery' '@bootstrap_js' '@PixeloidAppBundle/Resources/public/js/*' '@PixeloidAppBundle/Resources/public/lib/iCheck/icheck.min.js' '@PixeloidAppBundle/Resources/public/lib/jquery.easing.min.js' '@PixeloidAppBundle/Resources/public/lib/bootstrap-select.js' '@PixeloidAppBundle/Resources/public/lib/bootstrap-datepicker.js' '@PixeloidAppBundle/Resources/public/lib/bootstrap-datepicker.hu.js' %}
<script type="text/javascript" src="{{ asset_url }}"></script>
{% endjavascripts %}
</body>
</html>