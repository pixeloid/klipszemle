<!DOCTYPE html>
<html>
    <head>
        <title>{% block title %}Welcome!{% endblock %}</title>
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <!-- Bootstrap -->
        {% block stylesheets %}
            {% stylesheets  '@PixeloidAppBundle/Resources/public/fonts/font.css' filter='less, css_url_rewrite' output='css/fonts.css' %}
            <link href="{{ asset_url }}" type="text/css" rel="stylesheet" />
        {% endstylesheets %}
        {% stylesheets
        '@BraincraftedBootstrapBundle/Resources/less/form.less'
        '@PixeloidAppBundle/Resources/public/less/styles.less'
        filter='less, css_url_rewrite' output='css/styles.css' %}
        <link href="{{ asset_url }}" type="text/css" rel="stylesheet" media="all"/>
    {% endstylesheets %}
{% endblock %}
<!-- HTML5 Shim and Respond.js add IE8 support of HTML5 elements and media queries -->
{% include 'BraincraftedBootstrapBundle::ie8-support.html.twig' %}
</head>
<body>
    <nav class="hidden-print navbar-xs" role="navigation">
        <div class="container">
            {{ knp_menu_render('PixeloidAppBundle:Builder:topMenu', {'nav_type': '', 'currentAsLink': true, 'currentClass': 'active'}) }}
        </div>
    </nav>


    <nav class="navbar navbar-default">

    <header class="hidden-print">
        <div class="container">
            <div class="row">
                <div class="col-md-8"><a href="#"><h2><span>Magyar Gyermek-gasztroenterológiai Társaság <br><b> V. Kongresszusa</b></span></h2></a></div>
                <div class="col-md-4 text-right date"><h4><b>2015. október 1-3.</b> <span>Mátraháza – Lifestyle Hotel Mátra</span></h4></div>
            </div>
        </div>
    </header>

        <div class="container">



            <!-- Brand and toggle get grouped for better mobile display -->
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                </button>
            </div>
            <!-- Collect the nav links, forms, and other content for toggling -->
            <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                <div class="">
                    {{ knp_menu_render('PixeloidAppBundle:Builder:mainMenu', {'nav_type': '', 'currentAsLink': true, 'currentClass': 'active'}) }}
                </div>
            </div>
        </div>
    </nav>

<div class="wrapper">
    <div class="container container_main">
        
        {% block body %}{% endblock %}
    </div>
</div>
